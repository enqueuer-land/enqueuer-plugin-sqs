{
  "name": "enqueuer-plugin-sqs",
  "version": "0.0.1",
  "description": "Enqueuer sqs plugin",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "all": "npm run clean && npm run lint && npm run build && npm run test",
    "build": "tsc",
    "test": "nqr -a examples/sqs.yml -l `pwd` conf/nqr.yml",
    "clean": "rm -rf dist",
    "lint": "node_modules/.bin/tslint --project tsconfig.json",
    "prepublishOnly": "npm run all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/enqueuer-land/enqueuer-plugin-sqs.git"
  },
  "keywords": [
    "enqueuer",
    "nqr",
    "sqs",
    "plugin"
  ],
  "author": "Virgs",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/enqueuer-land/enqueuer-plugin-sqs/issues"
  },
  "homepage": "https://github.com/enqueuer-land/enqueuer-plugin-sqs#readme",
  "devDependencies": {
    "@types/node": "^11.9.5",
    "ts-node": "^8.0.2",
    "tslint": "^5.13.0",
    "typescript": "^3.3.3333"
  },
  "dependencies": {
    "aws-sdk": "^2.411.0",
    "enqueuer-plugins-template": "^1.0.7"
  }
}
